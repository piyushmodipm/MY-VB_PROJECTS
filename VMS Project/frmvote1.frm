Private Sub cmdAdd_Click()
Adodc2.Recordset.Update
Adodc1.Recordset.Update
If Label10.Caption = "yes" Then
MsgBox "you have already voted"
Else
Label9.Caption = Label9.Caption + 1
Label10.Caption = "yes"
Adodc2.Recordset.Fields("vote") = Label10.Caption
Adodc1.Recordset.Fields("Votes") = Label9.Caption
DataGrid1.Refresh
DataGrid2.Refresh
Adodc2.Recordset.Update
Adodc1.Recordset.Update
MsgBox "Vote has been submitted successfully"
End If
End Sub

